<h1><%= @user.email %></h1>

<ul>
    <li><%= @user.email %></li>
</ul>

<% if @user.subs %>
    <h2>Subs</h2>
        <ul>
            <% @user.subs.each do |sub| %>
                <li><a href="<%= sub_url(sub) %>"><%= sub.title %></a></li>
            <% end %>
        </ul>
<% end %>
<a href="<%= users_url %>">View all users</a>

